{
  "name": "zabbix-whatsapp-bot",
  "version": "1.0.0",
  "description": "Dispara alertas do Zabbix para grupo do WhatsApp",
  "main": "index.js",
  "scripts": {
    "start": "node validate-config.js && node index.js",
    "dev": "nodemon index.js",
    "setup": "node setup.js",
    "validate": "node validate-config.js",
    "pm2:start": "npm run validate && npm run logs:create && pm2 start ecosystem.config.js",
    "start:complete": "bash scripts/start-complete-system.sh",
    "pm2:stop": "pm2 stop all",
    "pm2:restart": "pm2 restart all",
    "pm2:logs": "pm2 logs",
    "pm2:logs:bot": "pm2 logs zabbix-whatsapp-bot",
    "pm2:logs:web": "pm2 logs voetur-web-admin",
    "pm2:monit": "pm2 monit",
    "pm2:delete": "pm2 delete all",
    "test:health": "curl -f http://localhost:3000/health || echo 'Serviço não está rodando'",
    "test:zabbix": "node scripts/test-zabbix.js",
    "test:server": "node scripts/test-server.js",
    "test:integration": "node scripts/test-zabbix-integration.js",
    "test:cnpj": "node scripts/test-cnpj.js",
    "test:cnpj:quick": "node scripts/test-cnpj-quick.js",
    "test:cnpj:real": "node scripts/test-cnpj-real.js",
    "test:cnpj:apis": "node scripts/test-cnpj-apis.js",
    "test:menu": "node scripts/test-menu.js",
    "web:start": "node web-admin/server.js",
    "web:dev": "nodemon web-admin/server.js",
    "web:reset": "bash scripts/reset-database.sh",
    "test:web": "bash scripts/test-web-panel.sh",
    "logs:create": "mkdir -p logs",
    "logs:clean": "rm -rf logs/*.log",
    "clean": "rm -rf node_modules wwebjs_auth .wwebjs_cache firstMessages.json logs"
  },
  "keywords": [
    "whatsapp",
    "zabbix",
    "bot"
  ],
  "author": "Kaique Rodrigues (kaiqueadm)",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.1.4",
    "express": "^4.18.2",
    "express-rate-limit": "^6.10.0",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "pino": "^8.16.0",
    "pino-pretty": "^10.2.0",
    "qrcode-terminal": "^0.12.0",
    "socket.io": "^4.7.2",
    "sqlite3": "^5.1.6",
    "whatsapp-web.js": "^1.34.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}
